<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Nick Lundin</Authors>
    <Company />
    <Product>Microsoft-DI-Adapters</Product>
    <Description>A library for bridging the gap between the build in .NET di framework and other di frameworks</Description>
    <PackageProjectUrl>https://github.com/nicklundin08/Microsoft-DI-Adapters</PackageProjectUrl>
    <RepositoryUrl>https://github.com/nicklundin08/Microsoft-DI-Adapters</RepositoryUrl>
    <PackageTags>Ninject Microsoft DependencyInjection ServiceCollection ServiceProvider</PackageTags>
    <PackageId>Microsoft-DI-Adapters</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.2.0" />
    <PackageReference Include="Ninject" Version="3.3.4" />
  </ItemGroup></Project>